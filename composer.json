{
	"name": "osmuhin/html-meta",
	"type": "library",
	"version": "1.0.0",
	"license": "MIT",
	"authors": [
		{
			"name": "Osmuhin Daniil",
			"email": "wischerdson@gmail.com"
		}
	],
	"homepage": "https://github.com/wischerdson/html-meta",
	"support": {
		"issues": "https://github.com/wischerdson/html-meta/issues",
		"source": "https://github.com/wischerdson/html-meta"
	},
	"require": {
		"php": ">=8.2",
		"ext-mbstring": "*",
		"guzzlehttp/guzzle": ">=6.3",
		"symfony/dom-crawler": ">=6.0",
		"masterminds/html5": ">=2.8",
		"symfony/mime": "*"
	},
	"require-dev": {
		"mockery/mockery": "^1.6.12",
		"phpunit/phpunit": "^11.5",
		"symfony/var-dumper": "^7.1"
	},
	"autoload": {
		"psr-4": {
			"Osmuhin\\HtmlMeta\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	},
	"scripts": {
		"test:phpunit": [
			"@putenv XDEBUG_MODE=coverage",
			"phpunit --colors=always"
		]
	}
}
